{
  "name": "hilal_web",
  "short_name": "Hilal",
  "id": "/",
  "description": "Hilal location time ",
  "start_url": "/hilal_web/",
  "scope": "/hilal_web/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#008080",
  "orientation": "portrait-primary",

  "icons": [
    {
      "src": "icons/Icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icons/Icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],

  "shortcuts": [
    {
      "name": "Open Hilal Locs.",
      "short_name": "Hilal",
      "description": "Quick access to prayer times",
      "url": "/hilal_web/",
      "icons": [{ "src": "icons/Icon-192.png", "sizes": "192x192" }]
    },
    {
      "name": "Settings",
      "short_name": "Settings",
      "description": "Open the settings page",
      "url": "/hilal_web/settings",
      "icons": [{ "src": "icons/Icon-192.png", "sizes": "192x192" }]
    }
  ],

  "screenshots": [
    {
      "src": "Icons/screenshot-wide.png",
      "sizes": "1362x460",
      "type": "image/png"
    },
    {
      "src": "Icons/screenshot-mobile.png",
      "sizes": "960x1280",
      "type": "image/png"
    }
  ],

  "protocol_handlers": [
    {
      "protocol": "web+hilal",
      "url": "/hilal_web/?link=%s"
    }
  ]
}
